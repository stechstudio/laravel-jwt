{
    "name": "stechstudio/laravel-jwt",
    "description": "Helper package that makes it easy to generate, consume, and protect routes with JWT tokens in Laravel",
    "type": "library",
    "require": {
        "php": "^8.1|^8.2",
        "lcobucci/jwt": "^4.3",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "~9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph",
            "email": "joseph@stechstudio.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "STS\\JWT\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\JWT\\JWTServiceProvider"
            ],
            "aliases": {
                "JWT": "STS\\JWT\\Facades\\JWT"
            }
        }
    }
}
